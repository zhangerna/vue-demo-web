y\" style=\"\">\n            <div class=\"pb-3 text-secondary\">Invite new members to <strong> <span\n                id=\"storeName\">Los Angeles Parking Lot</span> </strong></div>\n            <div class=\"pb-3\" style=\"font-size: 12px\">Send Invitations to :</div>\n            <div class=\"form-input\">\n              <div class=\"form-group col-md-6\" v-for=\"(item, i) in emails\">\n                <b-input-group class=\"mt-3\">\n                  <b-input-group-text slot=\"prepend\"><i class=\"fa fa-envelope\"></i></b-input-group-text>\n                  <b-form-input v-model=\"item.name\" type=\"email\" @change=\"verifyEmail(item.name)\"></b-form-input>\n                </b-input-group>\n              </div>\n            </div>\n            <div class=\"form-group col-md-8\" id=\"plusUserDiv\" style=\"margin-top:-48px;margin-left:48%\">\n              <div class=\"input-group-append col-md-4\" @click=\"addInputBox\">\n                    <span class=\"input-group-text\" id=\"plusUserBtn\">\n                      <i class=\"fa fa-plus\"></i>\n                    </span>\n              </div>\n            </div>\n            <div class=\"form-group form-actions col-6\">\n              <b-button variant=\"success\" @click=\"getAllEmails\" :disabled=\"btnActive\">Submit</b-button>\n            </div>\n          </div>\n\n\n          <div v-if=\"submitSuccess\" class=\"card-body\" style=\"\">\n            <div id=\"success_body\" class=\"row justify-content-center align-items-center\" style=\"\">\n              <div class=\"col-md-8 col-lg-6\">\n                <div class=\" mx-4\">\n                  <div class=\" px-4 align-items-center text-center\">\n                    <p style=\"font-size: 144px\"><i class=\"cui cui-check\"></i></p>\n                    <p class=\"text-muted\">Congratulations, you have successfully invited the users.</p>\n                    <div class=\"row\">\n                      <div class=\"col-6\">\n                        <button @click=\"$router.push('/index')\" class=\"btn btn-block btn-outline-primary\" type=\"button\">\n                          Dashboard\n                        </button>\n                      </div>\n                      <div class=\"col-6\">\n                        <button class=\"btn btn-block btn-outline-primary\" @click=\"continueInvite\" type=\"button\">\n                          Continue\n                        </button>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n\n        </div>\n      </div>\n\n      <Alert @closeRemindBox='closeBox' :buttonText=\"showAlerts.buttonText\" :show=\"showAlerts.show\"\n             :title=\"showAlerts.title\" :content=\"showAlerts.content\">\n      </Alert>\n\n    </div>\n\n\n  </div>\n</template>\n<script>\n\timport httpClient from '@/utils/httpclient';\n\timport Alert from '@components/model'\n\n\texport default {\n\t\tname: 'adminInvite',\n\t\tcomponents: {\n\t\t\tAlert\n\t\t},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\temails: [\n\t\t\t\t\t{\n\t\t\t\t\t\tname: ''\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tname: '',\n\t\t\t\t\t}\n\t\t\t\t],\n\t\t\t\tbtnActive: true,\n\t\t\t\tsubmitSuccess: false,\n\t\t\t\tshowAlerts: {\n\t\t\t\t\ttitle: '',\n\t\t\t\t\tcontent: '',\n\t\t\t\t\tshow: false,\n\t\t\t\t\tbuttonText: ''\n\t\t\t\t}\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\taddInputBox() {\n\t\t\t\tthis.emails.push({name: ''});\n\t\t\t},\n\t\t\tgetAllEmails() {\n\t\t\t\tlet emailsArray = [];\n\t\t\t\tlet url = 'https://api-saas-qa.zus.ai/api/v1/inviteUser';\n\t\t\t\tthis.emails.map((item, i) => {\n\t\t\t\t\titem.name.length ? emailsArray.push(item.name) : '';\n\t\t\t\t});\n\t\t\t\thttpClient.post(url, {emails: emailsArray.join(',')}).then((res) => {\n\t\t\t\t\tif (res.data.code === 200) {\n\t\t\t\t\t\tthis.submitSuccess = !this.submitSuccess\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t},\n\t\t\tverifyEmail(m) {//验证输入值，合法才可以提交\n\t\t