">\n      <div class=\"col-12\">\n        <div class=\"card\">\n          <div class=\"card-header\">\n            <strong>User Operation History</strong>\n            <small class=\"text-muted last-update\"></small>\n          </div>\n          <div class=\"card-body\">\n            <div class=\"row\">\n              <div class=\"col-sm-3\" v-for=\"item in userInfo\">\n                <div class=\"callout callout-secondary\">\n                  <small class=\"text-muted\">{{item.title}}</small>\n                  <br>\n                  <strong class=\"h5\" id=\"name-text\">{{item.value}} </strong>\n                </div>\n              </div>\n            </div>\n            <div class=\"card-body\">\n              <b-table class=\"user-table-list\" hover :items=\"data\" :fields=\"fields\">\n                <template slot=\"operation\" slot-scope=\"data\">\n                  <span>{{ data.item.type}}</span>\n                </template>\n                <template slot=\"vehicle\" slot-scope=\"data\">\n                  <a class=\"btn-md link-detail\" @click=\"$router.push('/vehicle-detail/' + data.item.vehicleId)\">\n                    {{data.item.vehicle}}\n                  </a>\n                </template>\n                <template slot=\"time\" slot-scope=\"data\">\n                  <div class=\"btn-md\">{{data.item.time}}</div>\n                </template>\n              </b-table>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-sm-12 col-md-5\">\n                <div class=\"dataTables_info\" id=\"DataTables0\" role=\"status\" aria-live=\"polite\">\n                  <span v-if=\"totalPages > 0 \">Showing {{10 * (currentPage - 1) + 1 }} to {{currentPage === totalPages? rows :currentPage * 10 }} of {{rows}} entries</span>\n                  <span v-else>Showing 0 to 0 of {{rows}} entries</span>\n                </div>\n              </div>\n              <div class=\"col-sm-12 col-md-7\">\n                <b-pagination align=\"right\" :total-rows=\"rows\" v-model=\"currentPage\" :per-page=\"perPage\" @input=\" turnToPage(currentPage)\"></b-pagination>\n              </div>\n            </div>\n\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\n\timport httpClient from '@/utils/httpclient';\n\timport {getDateString} from '@/utils/date';\n\n\texport default {\n\t\tname: 'adminDetail',\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tfields: {\n\t\t\t\t\toperation: {\n\t\t\t\t\t\tkey: 'operationType',\n\t\t\t\t\t\tlabel: 'Operation'\n\t\t\t\t\t},\n\t\t\t\t\tvehicle: {\n\t\t\t\t\t\tkey: 'vehicle',\n\t\t\t\t\t\tlabel: 'Vehicle'\n\t\t\t\t\t},\n\t\t\t\t\ttime: {\n\t\t\t\t\t\tkey: 'time',\n\t\t\t\t\t\tlabel: 'Time'\n\t\t\t\t\t},\n\t\t\t\t},\n\t\t\t\titems: null,\n\t\t\t\tdata: [],\n\t\t\t\tid: this.$route.params.id,\n\t\t\t\tcurrentPage: 1,\n\t\t\t\trows: 1,\n\t\t\t\tperPage: 1,\n\t\t\t\ttotalPages: 1,\n\t\t\t\tname: '',\n\t\t\t\tphone: '',\n\t\t\t\temail: '',\n\t\t\t\tstatus: '',\n\t\t\t\tuserInfo: [\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: 'Name',\n\t\t\t\t\t\tvalue: ''\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: 'Phone',\n\t\t\t\t\t\tvalue: ''\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: 'Email',\n\t\t\t\t\t\tvalue: ''\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: 'Status',\n\t\t\t\t\t\tvalue: ''\n\t\t\t\t\t},\n\t\t\t\t]\n\t\t\t}\n\t\t},\n\t\tmounted() {\n\t\t\tlet userInfoLink = '/api/v1/getUserById';\n\t\t\thttpClient.post(userInfoLink, {userId: this.id}).then((res) => {\n\t\t\t\tif (res.data.code === 200) {\n\t\t\t\t\tlet myData = res.data.data;\n\t\t\t\t\tthis.userInfo[0].value = myData.lastName + ' ' + myData.firstName;\n\t\t\t\t\tthis.userInfo[1].value = myData.phone;\n\t\t\t\t\tthis.userInfo[2].value = myData.userEmail;\n\t\t\t\t\tthis.userInfo[3].value = myData.status === 1 ? 'Active' : 'InActive';\n\t\t\t\t}\n\t\t\t});\n\n\t\t\tthis.getList(1)\n\t\t},\n\t\tmethods: {\n\t\t\tgetList(num) {\n\t\t\t\tlet userCarListLink = '/api/v1/operationlog/listbyuserid?userId=' + this.id + '&pageNo=' + num;\n\t\t\t\thttpClient.get(userCarListLink).then((res) => {\n\t\t\t\t\tlet myData = res.data.data.datas;\n\t\t\t\t\tthis.items = myData;\n\t\t\t\t\tthis.currentPage = res.data.data.pageNo;\n\t\t\t\t\tthis.perPage = res.data.data.pageSize;\n\t\t\t\t\tthis.totalPages = res.data.data.totalPages;\n\t\t\t\t\tthis.rows = res.data.data.rowCount;\n\t\t\t\t\tlet myArr = [];\n\t\t\t\t\tmyData.map((item, i) => {\n\t\t\t\t\t\titem.year = item.year ? item.year : 'N/A';\n\t\t\t\t\t\titem.make = item.make ? item.make : 'N/A';\n\t\t\t\t\t\titem.model = item.model ? item.model : 'N/A';\n\t\t\t\t\t\tmyArr.push({\n\t\t\t\t\t\t\toperationType: item.operationType,\n\t\t\t\t\t\t\tvehicle: item.year + ' ' + item.make + ' ' + item.model,\n\t\t\t\t\t\t\ttime: getDateString(item.createTime),\n\t\t\t\t\t\t\tuserId: item.userId,\n\t\t\t\t      vehicleId:item.vehicleId\n\t\t\t\t\t\t});\n\t\t\t\t\t});\n\t\t\t\t\tthis.data = myArr;\n\t\t\t\t})\n\t\t\t},\n\t\t\tturnToPage(p) {\n\t\t\t    console.log(p);\n\t\t\t\tthis.getList(p);\n\t\t\t}\n\t\t}\n\t}\n</script>\n<style lang=\"scss\" scoped>\n  .user-table-list {\n    .link-detail {\n      color: #20a8d8;\n      cursor: pointer;\n\n      &:hover {\n        text-decoration: underline;\n        color: #167495;\n      }\n    }\n  }\n\n</style>"]}]}