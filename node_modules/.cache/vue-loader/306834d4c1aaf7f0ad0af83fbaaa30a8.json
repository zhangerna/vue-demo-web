    }\n    }\n}\n",{"version":3,"sources":["shopDetail.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiNA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"shopDetail.vue","sourceRoot":"src/views/dashboard/inventory","sourcesContent":["<template>\n    <div class=\"animated fadeIn shop-detail\">\n        <b-card>\n            <div slot=\"header\">\n                {{ $t('m.add_shop.manager_detail') }}\n                <button class=\"btn btn-info btn-sm active float-right font-weight-bold color-white\"\n                        @click=\"$router.push('/inventory/shop-manager/edit/' + $route.params.id)\">{{ $t('m.common.edit') }}\n                </button>\n            </div>\n            <b-row>\n                <b-col sm=\"6\" class=\"left\">\n                    <div class=\"border\">\n                        <b-row>\n                            <b-col sm=\"12\" lg=\"12\" class=\"title\">\n                                {{ data.storeName }}\n                            </b-col>\n                        </b-row>\n                        <b-row>\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.country') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.country | checkNan }}\n                            </b-col>\n                        </b-row>\n                        <b-row>\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.state') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.state | checkNan }}\n                            </b-col>\n                        </b-row>\n                        <b-row>\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.city') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.city | checkNan }}\n                            </b-col>\n                        </b-row>\n                        <b-row v-if=\"GLOBAL.APP_ENV == 'cn'\">\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.area') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.area | checkNan }}\n                            </b-col>\n                        </b-row>\n                        <b-row>\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.street') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.streetAddress }}\n                            </b-col>\n                        </b-row>\n                        <b-row>\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.address') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.storeAddress }}\n                            </b-col>\n                        </b-row>\n                        <b-row>\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.zip') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.zipCode }}\n                            </b-col>\n                        </b-row>\n                        <b-row>\n                            <b-col sm=\"4\" lg=\"4\">\n                                {{ $t('m.add_shop.stock') }}\n                            </b-col>\n                            <b-col sm=\"8\" lg=\"8\">\n                                {{ data.vehicleInventory }}\n                            </b-col>\n                        </b-row>\n                        <template v-for=\"(item, key) in data.userList\">\n                            <div :key=\"key\">\n                                <b-row>\n                                    <b-col sm=\"4\" lg=\"4\">\n                                        {{ $t('m.add_shop.manager') }}\n                                    </b-col>\n                                    <b-col sm=\"8\" lg=\"8\">\n                                        {{ filterLastFirstName($i18n.locale, item.firstName, item.lastName) }}\n                                    </b-col>\n                                </b-row>\n                                <b-row>\n                                    <b-col sm=\"4\" lg=\"4\">\n                                        {{ $t('m.admin.email') }}\n                                    </b-col>\n                                    <b-col sm=\"8\" lg=\"8\">\n                                        {{ item.userEmail }}\n                                    </b-col>\n                                </b-row>\n                                <b-row>\n                                    <b-col sm=\"4\" lg=\"4\">\n                                        {{ $t('m.common.contact_phone') }}\n                                    </b-col>\n                                    <b-col sm=\"8\" lg=\"8\">\n                                        {{ item.userPhone }}\n                                    </b-col>\n                                </b-row>\n                            </div>\n                        </template>\n                    </div>\n                </b-col>\n                <b-col sm=\"6\" class=\"right\">\n                    <my-map :address=\"address\"></my-map>\n                </b-col>\n            </b-row>\n            <div class=\"del-group text-center\">\n                <p>{{ $t('m.detail_shop.del_p') }}</p>\n                <button class=\"btn btn-danger\" @click=\"removeData(data.id)\">{{ $t('m.detail_shop.del_b') }}</button>\n            </div>\n        </b-card>\n        <model\n                :show=\"showTip\"\n                :title=\"title\"\n                :content=\"content\"\n                :buttonText=\"btnGroup\"\n                @closeRemindBox=\"closeBox\"\n        ></model>\n    </div>\n</template>\n\n\n<script>\n    import httpclient from '@/utils/httpclient';\n    import myMap from \"@/components/mapForAddress\";\n    import model from \"@/components/model\";\n    import { filterLastFirstName, checkNullString } from \"@/utils/db\";\n    export default {\n        name: \"shopDetail\",\n        components: {\n            myMap,\n            model\n        },\n        data() {\n            return {\n                address: \"\",\n                data: {},\n                showTip: false,\n                title: '',\n                content: '',\n                curDel: null,\n                btnGroup: {},\n                times: 0\n            }\n        },\n        mounted() {\n            if (this.$route.params.id) {\n                httpclient.get(\"/api/v1/store/detail?storeId=\" + this.$route.params.id, {\n                }).then((res) => {\n                    if (res.data.code == 200) {\n                        let respones = res.data.data;\n                        this.data = respones;\n                        this.address = checkNullString(respones.country) + \" \" + checkNullString(respones.state) + \" \" + checkNullString(respones.city) + \" \" +\n                                checkNullString(respones.area) + \" \" + checkNullString(respones.streetAddress) + \" \" + checkNullString(respones.storeAddress)\n                    }\n                });\n            }\n        },\n        methods: {\n            filterLastFirstName,\n            closeBox(type) {\n                if (type) {\n                    this.times = this.times + 1;\n                    this.title = this.$t('m.tip.warn');\n                    this.content = this.$t('m.detail_shop.del2');\n                    if (this.times == 2) {\n                        httpclient.post(\"/api/v1/store/delete\", {\n                            storeId: this.curDel\n                        }).then((res) => {\n                            if (res.data.code == 200 && res.data.data.result) {\n                                this.$router.push(\"/shop-manager\")\n                            } else {\n                                this.showTip = true;\n                                this.title = this.$t('m.tip.warn');\n                                this.content = this.$t('m.edit_shop.no_delete');\n                                this.btnGroup = {btnFirst: this.$t('m.tip.b_cancel')}\n                            }\n                        }).catch(err => {\n                            this.showTip = false;\n                            err\n                        });\n                    }\n                } else {\n                    this.showTip = false;\n                }\n            },\n            removeData(id) {\n                this.times = 0;\n                this.curDel = id;\n                this.showTip = true;\n                this.title = this.$t('m.tip.warn');\n                this.content = this.$t('m.detail_shop.del1');\n                this.btnGroup = {btnFirst: this.$t('m.tip.b_cancel'), btnSecond: this.$t('m.tip.b_ok')}\n            }\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n\n    .shop-detail {\n        .row {\n            padding: .75rem 1.25rem;\n        }\n\n        .title {\n            font-size: 18px;\n            color: #63c2de;\n        }\n\n        .card {\n            margin: 0 auto;\n        }\n\n        .del-group {\n            padding: 4px 0;\n            margin-bottom: 20px;\n        }\n\n        .left {\n            padding: 10px;\n            padding-top: 0;\n            .row {\n                margin: 0px;\n                border-bottom: 1px solid #23282c;\n                padding: 10px;\n            }\n\n            .row > div {\n                padding-left: 4px;\n            }\n        }\n    }\n</style>\n"]}]}