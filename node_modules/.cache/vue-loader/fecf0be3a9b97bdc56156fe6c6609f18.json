List.map((item, index) => {\n                                        if (item.proviceName == this.getState) {\n                                            this.state = index + 1;\n                                            item.cityList.map((item, index) => {\n                                                if (item.cityName == this.getCity) {\n                                                    this.city = index + 1;\n                                                    item.areaDTOList.map((item, index) => {\n                                                        if (item.areaName == this.getArea) {\n                                                            this.area = index + 1;\n                                                        }\n                                                    });\n                                                }\n                                            });\n                                        }\n                                    });\n                                }\n                            });\n                        }\n                    });\n                    for (let item of res.data.data.userList) {\n                        this.managerList.push({\n                            email: item.userEmail,\n                            phone: item.userPhone,\n                            lastName: item.lastName,\n                            firstName: item.firstName,\n                            ori: true\n                        });\n                    }\n                    this.managerList.length == 0\n                        ? this.managerList.push({\n                            email: \"\",\n                            phone: \"\",\n                            lastName: \"\",\n                            firstName: \"\"\n                        })\n                        : \"\";\n                    this.name = res.data.data.storeName;\n                    this.street = res.data.data.streetAddress;\n         